{
  "name": "react_mp_2017",
  "version": "1.0.1",
  "description": "EPAM React Mentoring Program",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run clear && npm run watch_webpack\" \"npm run babelClient\" \"npm run babelServer\" \"npm run watch_server\"",
    "build": "webpack -p",
    "watch_webpack": "webpack --watch",
    "watch_server": "nodemon babeled/server/server.js",
    "clear": "rimraf dist && rimraf babeled",
    "babelClient": "node_modules/.bin/babel src -d babeled/src --watch",
    "babelServer": "node_modules/.bin/babel server -d babeled/server --watch",
    "test": "jest --watch --coverage"
  },
  "keywords": [
    "react"
  ],
  "author": "Aliaksei Habruseu",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "express": "^4.15.4",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "json-loader": "^0.5.7",
    "nodemon": "^1.12.1",
    "raf-polyfill": "^1.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.0.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "ejs": "^2.5.7",
    "es6-promise": "^4.1.1",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-config": "^1.0.0-beta.4",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "rimraf": "^2.6.2",
    "styled-components": "^2.2.1"
  }
}
